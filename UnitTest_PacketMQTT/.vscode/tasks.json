{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "C/C++: cpp build active file ARM",
      "command": "/usr/bin/arm-linux-gnueabihf-g++-10",
      "args": [
        "-std=c++2a",
        "-g",
        "-Wall",
        "-I${workspaceFolder}",
        "-I${workspaceFolder}/../LIB.Utils",
        "${workspaceFolder}/main.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsPacketMQTT.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsPacketMQTT_Test.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsTest.cpp",
        "-o",
        "${workspaceFolder}/unit_test_packet_mqtt",
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "/usr/bin/g++-11",
      "args": [
        "-fdiagnostics-color=always",
        "-std=c++2a",
        "-g",
        "-Wall",
        "-I${workspaceFolder}",
        "-I${workspaceFolder}/../LIB.Utils",
        "${workspaceFolder}/main.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsPacketMQTT.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsPacketMQTT_Test.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsTest.cpp",
        "-o",
        "${workspaceFolder}/unit_test_packet_mqtt_dbg",
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "compiler: /usr/bin/g++"
    }
  ]
}